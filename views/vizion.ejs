<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Open HTML</title>
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Specify the URL of the HTML file you want to open
            var url = 'vizion.html'; // Change this to the path of the HTML file you want to open
            
            // Open the URL in a new tab
            var newTab = window.open(url, '_blank');
            if (newTab) {
                newTab.focus();
            } else {
                console.error('Failed to open new tab. It might be blocked by a popup blocker.');
            }
        });
    </script>
</head>
<body>
</body>
</html>
